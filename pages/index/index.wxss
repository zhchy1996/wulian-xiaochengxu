/**index.wxss**/
.container{
  display: flex;
  height: 100vh;
}
.head{
  width:100%;
  border-bottom:1px solid #666;
  text-align: center;
  padding:10rpx 0;
  font-size:30px;
}
.wrapper{
  display: flex;
  flex-direction: column;
  width:80vw;
  justify-content: space-around;
  height:100%;
}
.wrapper view{
  display: flex;
  justify-content: space-between;
  text-align: center;
  margin-bottom:100rpx;
}
.wrapper view navigator{
    width:200rpx;
    padding:10rpx;
}
.wrapper image{
  display: block;
  width:100rpx;
  height:100rpx;
  margin:30rpx auto;
}
/* nav-bar开始 */
.nav-bar{
  display: flex;
  justify-content: space-around;
  width:100vw;
  border-top:2px solid #666;
}
.nav-bar navigator{
  width:25%;
  text-align: center;
  border-right: 1px solid #666;
  padding-bottom: 10rpx;
}
.nav-bar navigator:first-child{
  color:#1296db;
}
.nav-bar image{
  display: block;
  width:100rpx;
  height:100rpx;
  margin: 0 auto;
}
.nav-bar navigator:last-child{
  border-right:0px;
}